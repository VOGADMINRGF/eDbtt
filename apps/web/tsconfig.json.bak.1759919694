{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@core/triMongo": [
        "../../core/db/triMongo.ts"
      ],
      "@/*": [
        "src/*",
        "apps/web/src/*"
      ],
      "@features/*": [
        "../../features/*"
      ],
      "@core/*": [
        "../../core/*",
        "core/*"
      ],
      "@db-web": [
        "../../packages/db-web/src/client.ts"
      ],
      "@db-web/*": [
        "../../packages/db-web/src/*"
      ],
      "@vog/ui": [
        "../../packages/ui/dist/index.d.ts"
      ],
      "@vog/ui/*": [
        "../../packages/ui/dist/*"
      ]
    },
    "moduleResolution": "Bundler",
    "incremental": true,
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "types": [
      "node"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "src/**/*",
    "types/**/*.d.ts",
    "../../features/**/*",
    "../../core/**/*",
    "../../packages/ui/dist/**/*.d.ts",
    "next.config.*"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "dist",
    "coverage"
  ]
}