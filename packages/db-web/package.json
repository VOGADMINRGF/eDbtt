{
  "name": "@db/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "prisma/schema.prisma"
  ],
  "scripts": {
    "generate": "prisma generate --schema=prisma/schema.prisma",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "prisma": "latest",
    "typescript": "^5.6.0"
  },
  "sideEffects": false
}
